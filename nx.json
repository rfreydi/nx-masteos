  },
  "projects": {
    "workshop": { "tags": [] },
    "workshop-e2e": { "tags": [], "implicitDependencies": ["workshop"] },
    "basecamp": { "tags": [] },
    "basecamp-e2e": { "tags": [], "implicitDependencies": ["basecamp"] },
    "outpost": { "tags": [] },
    "headquarter": { "tags": [] }
  }
